{
  "manifest_version": 3,
  "name": "PsySymbol Anywhere",
  "version": "0.1.0",
  "description": "Decode symbols and numbers on any page \u2014 quick links to PsySymbol.",
  "action": {
    "default_title": "PsySymbol Anywhere"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}